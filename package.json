{
  "name": "cache-shield-sdk",
  "version": "1.2.1",
  "description": "ðŸ›¡ï¸ Clear browser caches from client-side with ease. Service Workers, Storage, IndexedDB, Cookies & more.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "browser": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react.js",
      "import": "./dist/react.mjs",
      "types": "./dist/react.d.ts"
    },
    "./vue": {
      "require": "./dist/vue.js",
      "import": "./dist/vue.mjs",
      "types": "./dist/vue.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build && npm test",
    "size": "size-limit",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "cache",
    "clear-cache",
    "browser-cache",
    "service-worker",
    "localstorage",
    "indexeddb",
    "cookies",
    "pwa",
    "cache-busting",
    "cache-invalidation",
    "client-side",
    "frontend",
    "sdk"
  ],
  "author": "Suneel Kumar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suneelkumarr/cache-shield-sdk.git"
  },
  "homepage": "https://cache-shield.dev",
  "bugs": {
    "url": "https://github.com/suneelkumarr/cache-shield-sdk/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@size-limit/preset-small-lib": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react": "^19.2.3",
    "rollup": "^4.0.0",
    "size-limit": "^8.0.0",
    "ts-jest": "^29.0.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "5 KB"
    }
  ]
}
