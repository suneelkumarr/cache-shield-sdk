{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "declaration": false,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "jsx": "react-jsx"
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules", 
    "dist",
    "tests"
  ]
}
